{
    "compilerOptions": {
        "strictNullChecks": true,
        "sourceMap": false,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "target": "es6",
        "jsx":"preserve",
        "allowSyntheticDefaultImports": true,
        "noUnusedParameters":true,
        "noUnusedLocals": true,
        "noImplicitAny": false,
        "lib": [
            "es7",
            "dom"
        ]
    },
    "exclude": [
      "node_modules",
      "lib",
      "es"
    ]
  }
